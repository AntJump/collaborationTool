<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
		PUBLIC "-//mybatis.org//DTD SQL Map Config 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- http://www.mybatis.org/mybatis-3/configuration.html#settings 참고 -->
	<settings>
		<!-- query 결과 컬럼의 값이 null일 경우 result에 null로 setting할지 여무 -->
		<setting name="callSettersOnNulls" value="true" />
		<!-- null parameter 허용 -->
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>

	<typeAliases>
		<typeAlias alias="MemberDto" type="com.antjump.ant.member.dto.MemberDto"/>
		<typeAlias alias="AdminDto" type="com.antjump.ant.admin.dto.AdminDto"/>
		<typeAlias alias="QNAListDto" type="com.antjump.ant.qna.dto.QNAListDTO"/>
		<typeAlias alias="IssueDto" type="com.antjump.ant.issue.dto.IssueDTO"/>
		<typeAlias alias="IssueReplyDto" type="com.antjump.ant.issue.dto.IssueReplyDTO"/>
		<typeAlias alias="SprintDto" type="com.antjump.ant.sprint.model.dto.SprintDto"/>
		<typeAlias alias="RemembranceDto" type="com.antjump.ant.remembrance.model.dto.RemembranceDto"/>

	</typeAliases>
	<mappers>
		<mapper resource="MemberMapper.xml" />
		<mapper resource="AdminMapper.xml" />
		<mapper resource="FAQMapper.xml" />
		<mapper resource="GoodsMapper.xml" />
		<mapper resource="PaymentMapper.xml" />
		<mapper resource="QNAMapper.xml" />
		<mapper resource="IssueMapper.xml" />
		<mapper resource="SprintMapper.xml" />
		<mapper resource="RemembranceMapper.xml" />
		<mapper resource="ProjectMapper.xml"/>
		<mapper resource="InvitationMapper.xml"/>

	</mappers>
</configuration>